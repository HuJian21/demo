extends layout

block content
  section.col-md-4.loginSection
      .panel.panel-default
        .panel-heading 用户注册              
        .panel-body
          form(method='post')
            .form-group
              label(for='username') 上传文件                        
              input#username.form-control(type='file', name='file')                                          
            button.btn.btn-primary(type='submit') 上传
    